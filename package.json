{
  "name": "ga-dashboard-plugin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "node-sass sass/style.scss -wo ./ && stylelint '*.css' --fix || true && stylelint '*.css' --fix & wp-scripts start",
    "compile:css": "node-sass sass/style.scss -o ./ && stylelint '*.css' --fix || true && stylelint '*.css' --fix",
    "compile:rtl": "rtlcss style.css style-rtl.css",
    "lint:scss": "wp-scripts lint-style 'sass/**/*.scss'",
    "lint:js": "wp-scripts lint-js 'js/*.js'",
    "compile:js": "wp-scripts build",
    "bundle": "dir-archiver --src . --dest ../_s.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist sass style.css.map",
    "deploy": "./scripts/deploy.sh"
  },
  "devDependencies": {
    "@wordpress/scripts": "^12.1.0",
    "dir-archiver": "^1.1.1",
    "node-sass": "^4.14.1",
    "rtlcss": "^2.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nadia89M/ga-dashboard-plugin.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nadia89M/ga-dashboard-plugin/issues"
  },
  "homepage": "https://github.com/Nadia89M/ga-dashboard-plugin#readme",
  "dependencies": {
    "dir-archiver": "^1.2.0"
  }
}
